{
    "$schema": "https://raw.githubusercontent.com/KonstantinDege/schemas/refs/heads/main/mission_schema.json",
    "parameter": {
        "flight_height": 15
    },
    "commands": [
        {
            "action": "takeoff",
            "commands": {
                "altitude": 15
            }
        },
        {
            "action": "start_camera",
            "commands": {
                "ips": 1
            }
        },
        {
            "action": "mov_multiple",
            "commands": [
                {
                    "lat": 0,
                    "lon": 0,
                    "yaw": 0
                },
                {
                    "lat": 4,
                    "lon": 4,
                    "yaw": 0
                }
            ]
        },
        {
            "action": "delay",
            "commands": {
                "time": 5
            }
        },
        {
            "action": "stop_camera"
        },
        {
            "action": "mov_to_objects_cap_pic",
            "commands": {
                "delay": 2
            }
        }
    ]
}