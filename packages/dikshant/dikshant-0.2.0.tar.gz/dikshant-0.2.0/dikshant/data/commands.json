{
    "commands": [
        {
            "action": "open_app",
            "patterns": [
                "open (?P<app>.+)",
                "start (?P<app>.+)",
                "launch (?P<app>.+)"
            ],
            "response": "Opening {app}"
        },
        {
            "action": "open_website",
            "patterns": [
                "open (?P<site>.+)",
                "go to (?P<site>.+)",
                "visit (?P<site>.+)"
            ],
            "response": "Opening {site}"
        },
        {
            "action": "create_folder",
            "patterns": [
                "create new folder",
                "make new folder"
            ],
            "response": "Created new folder on desktop"
        },
        {
            "action": "sleep",
            "patterns": [
                "take rest",
                "go to sleep"
            ],
            "response": "Goodbye"
        }
    ]
}