    "remote_commands": {
        "new_lot": {
            "rcmd": "new_lot",
            "params": [
                "lot_name", "lot_quality"
            ],
            "description": "eap开工单"
        },
        "pp_select": {
            "rcmd": "pp_select",
            "params": [
                "recipe_name"
            ],
            "description": "eap通知设备切换配方"
        },

        "LotStartReply": {
            "rcmd": "LotStartReply",
            "params": [
                "PPID", "Circulate_StockNo", "Circulate_No", "Circulate_item", "xxWorkOrder_Qty", "Circulate_Qty"
            ],
            "description": "eap监控到 1006 new_circulate 事件后下发的 LotStartReply"
        },
        "keyCarrierReply": {
            "rcmd": "keyCarrierReply",
            "params": [
                "CODE", "Circulate_No"
            ],
            "description": "eap监控到 2003 solder_carrier_in_request_uploading 回复回流焊托盘是否可以进站"
        },
        "START": {
            "rcmd": "START",
            "params": [
                "CODE", "Circulate_No"
            ],
            "description": "eap监控到 1011 product_in_request 回复基板产品是否可以做"
        },
        "BridgeInReply": {
            "rcmd": "BridgeInReply",
            "params": [
                "CODE", "Circulate_No"
            ],
            "description": "eap监控到 1012 bridge_in_request 回复半桥产品是否可以做"
        }
    },

    "signal_address": {
        "uploading_tag": {
            "mes_heart": {
                "description": "MES 心跳 2s 更新一次",
                "address": "Application.gvl_OPMODE01_MES.mes2plc.geneal.heartBeat",
                "data_type": "bool"
            },
            "control_state": {
                "description": "实时监控设备的控制状态",
                "address": "",
                "data_type": "int"
            },
            "machine_state": {
                "description": "实时监控设备的运行状态",
                "address": "Application.gvl_OPMODE01_MES.plc2mes.geneal.deviceState",
                "data_type": "int"
            },
            "alarm_id": {
                "description": "出现报警时, 报警id",
                "address": "Application.gvl_OPMODE01_MES.plc2mes.geneal.alarmCode",
                "data_type": "int"
            },
            "current_recipe_id": {
                "description": "当前配方id",
                "address": "Application.gvl_OPMODE01_MES.plc2mes.",
                "data_type": "int"
            },

            "new_lot": {
                "description": "开工单",
                "call_backs": [
                    {
                        "description": "写入工单状态, 开工单成功",
                        "operation_func": "write_specify_value",
                        "address": "Application.gvl_OPMODE01_MES.mes2plc.geneal.Lot_status",
                        "data_type": "int",
                        "value": 1
                    },
                    {
                        "description": "写入EAP下发的工单号",
                        "operation_func": "write_dv_value",
                        "address": "Application.gvl_OPMODE01_MES.mes2plc.geneal.Lot_name",
                        "data_type": "string",
                        "sv_name": "circulate_name"
                    },
                    {
                        "description": "写入EAP下发的工单数量",
                        "operation_func": "write_sv_value",
                        "address": "Application.gvl_OPMODE01_MES.mes2plc.geneal.Lot_quantity",
                        "data_type": "int",
                        "sv_name": "lot_quality"
                    },
                    {
                        "description": "写入工单状态, 允许运行",
                        "operation_func": "write_specify_value",
                        "address": "Application.gvl_OPMODE01_MES.mes2plc.geneal.Lot_status",
                        "data_type": "int",
                        "value": 3
                    }
                ]
            },
            "end_lot": {
                "description": "结工单",
                "call_backs": [
                    {
                        "description": "写入工单状态",
                        "operation_func": "write_specify_value",
                        "address": "Application.gvl_OPMODE01_MES.mes2plc.geneal.Lot_status",
                        "data_type": "int",
                        "value": 1,
                        "event_name": "end_lot"
                    }
                ]
            },
            "pp_select": {
                "description": "切换配方",
                "call_backs": [
                    {
                        "description": "写入要切换的配方id",
                        "operation_func": "write_sv_value",
                        "address": "Application.gvl_OPMODE01_MES.mes2plc.changeOver.recipeID",
                        "data_type": "int",
                        "sv_name": "pp_select_recipe_id"
                    },
                    {
                        "description": "写入要切换的配方指令",
                        "operation_func": "write_specify_value",
                        "address": "Application.gvl_OPMODE01_MES.mes2plc.changeOver.cmdID",
                        "data_type": "int",
                        "value": 2
                    },
                    {
                        "description": "通知 plc 切换配方信号",
                        "operation_func": "write_specify_value",
                        "address": "Application.gvl_OPMODE01_MES.mes2plc.changeOver.cmd",
                        "data_type": "bool",
                        "value": true
                    },
                    {
                        "description": "清空 通知 plc 切换配方信号",
                        "premise_address": "Application.gvl_OPMODE01_MES.plc2mes.changeOver.done",
                        "premise_value": true,
                        "premise_data_type": "bool",
                        "premise_time_out": 3,
                        "operation_func": "write_specify_value",
                        "address": "Application.gvl_OPMODE01_MES.mes2plc.changeOver.cmd",
                        "data_type": "bool",
                        "value": false
                    },
                    {
                        "description": "读取设备当前配方id",
                        "operation_func": "read_update_sv",
                        "address": "Application.gvl_OPMODE01_MES.plc2mes.changeOver.recipeID",
                        "data_type": "int",
                        "sv_name": "current_recipe_id"
                    }
                ]
            },

            "solder_carrier_in_request_uploading": {
                "description": "焊接托盘请求进入pinfin设备事件",
                "address": "Application.gvl_OPMODE01_MES.mes2plc.",
                "data_type": "int",
                "value": 1,
                "call_backs": [
                    {
                        "description": "清除 eap 回复了焊接托盘进站请求标识",
                        "operation_func": "clean_eap_reply",
                        "dv_name": "is_eap_reply_carrier_in_uploading"
                    },
                    {
                        "description": "读取焊接托盘码",
                        "operation_func": "read_update_dv",
                        "address": "Application.gvl_OPMODE01_MES.plc2mes.",
                        "data_type": "string",
                        "dv_name": "solder_carrier_code_in_pinfin",
                        "event_name": "solder_carrier_in_request_uploading"
                    },
                    {
                        "description": "等待 eap 回复焊接托盘是否允许进站",
                        "operation_func": "wait_eap_reply",
                        "dv_name": "is_eap_reply_carrier_in_uploading",
                        "not_allow_dv_name": "is_allow_solder_carrier_in_uploading",
                        "not_allow_dv_value": 2
                    },
                    {
                        "description": "写入 eap 是否允许焊接托盘进站",
                        "operation_func": "write_dv_value",
                        "address": "Application.gvl_OPMODE01_MES.plc2mes.",
                        "data_type": "int",
                        "dv_name": "is_allow_solder_carrier_in_uploading"
