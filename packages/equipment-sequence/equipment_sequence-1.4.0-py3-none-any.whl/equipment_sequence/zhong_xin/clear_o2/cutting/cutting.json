    "signal_address": {
        "mes_heart": {
            "description": "MES 心跳",
            "address": 92,
            "data_type": "bool",
            "bit_index": 0
        },
        "control_state": {
            "description": "控制状态",
            "address": 0,
            "data_type": "bool",
            "bit_index": 1
        },
        "machine_state": {
            "description": "设备运行状态",
            "address": 2,
            "data_type": "int",
            "size": 1
        },
        "current_recipe_id": {
            "description": "当前配方id",
            "address": 2,
            "data_type": "int",
            "size": 1
        },
        "alarm_code": {
            "description": "当运行状态是4alarm时读取报警代码",
            "address": 2,
            "data_type": "int",
            "size": 1
        },
        "pp_select": {
            "description": "EAP 切换配方",
            "call_back": [
                {
                    "description": "写入要切换的配方id",
                    "operation_func": "write_sv_value",
                    "address": 252,
                    "data_type": "int",
                    "size": 1,
                    "sv_name": "pp_select_recipe_id"
                },
                {
                    "description": "通知 plc 切换配方信号",
                    "operation_func": "write_specify_value",
                    "address": 250,
                    "data_type": "bool",
                    "bit_index": 0,
                    "value": true
                },
                {
                    "description": "清空 通知 plc 切换配方信号",
                    "premise_address": 246,
                    "premise_data_type": "bool",
                    "premise_bit_index": 0,
                    "premise_value": true,
                    "premise_size": 2,
                    "wait_time": 5,
                    "operation_func": "write_specify_value",
                    "address": 250,
                    "data_type": "bool",
                    "bit_index": 0,
                    "value": false
                },
                {
                    "description": "读取设备当前配方id",
                    "operation_func": "read_update_sv",
                    "address": 248,
                    "data_type": "int",
                    "size": 2,
                    "sv_name": "current_recipe_id"
                }
            ]
        },
        
        "carrier_in": {
            "loop": true,
            "description": "回流焊请求进站",
            "address": "2",
            "data_type": "int",
            "size": 1,
            "value": 1,
            "call_backs": [
                {
                    "description": "读取回流焊托盘码",
                    "operation_func": "read_update_dv",
                    "address": "2",
                    "data_type": "str",
                    "size": 10,
                    "dv_name": "carrier_code",
                    "event_name": "carrier_in"
                },
                {
                    "description": "写入回流焊请求进站信号MES反馈信号",
                    "operation_func": "write_specify_value",
                    "address": "D16011",
                    "data_type": "int",
                    "size": 1,
                    "value": 1
                },
                {
                    "description": "根据回流焊请求进站信号清空MES反馈信号",
                    "premise_address": "D16001",
                    "premise_data_type": "int",
                    "premise_size": 1,
                    "premise_value": 0,
                    "operation_func": "write_specify_value",
                    "address": "D16011",
                    "data_type": "int",
                    "size": 1,
                    "value": 0
                }
            ]
        },
        "carrier_out": {
            "loop": true,
            "description": "空的回流焊托盘出站",
            "address": "D16004",
            "data_type": "int",
            "size": 1,
            "value": 1,
            "call_backs": [
