    "signal_address": {
        "mes_heart": {
            "description": "MES 心跳",
            "address": 92,
            "data_type": "bool",
            "bit_index": 0
        },
        "control_state": {
            "description": "控制状态",
            "address": 0,
            "data_type": "bool",
            "bit_index": 1
        },
        "machine_state": {
            "description": "设备运行状态",
            "address": 2,
            "data_type": "int"
        },
        "current_recipe_id": {
            "description": "当前配方id",
            "address": 2,
            "data_type": "int"
        },
        "alarm_code": {
            "description": "当运行状态是4alarm时读取报警代码",
            "address": 2,
            "data_type": "int"
        },
        "pp_select": {
            "description": "EAP 切换配方",
            "call_back": [
                {
                    "description": "写入要切换的配方id",
                    "operation_func": "write_sv_value",
                    "address": 252,
                    "data_type": "int",
                    "sv_name": "pp_select_recipe_id"
                },
                {
                    "description": "通知 plc 切换配方信号",
                    "operation_func": "write_specify_value",
                    "address": 250,
                    "data_type": "bool",
                    "bit_index": 0,
                    "value": true
                },
                {
                    "description": "清空 通知 plc 切换配方信号",
                    "premise_address": 246,
                    "premise_data_type": "bool",
                    "premise_bit_index": 0,
                    "premise_value": true,
                    "premise_size": 2,
                    "wait_time": 5,
                    "operation_func": "write_specify_value",
                    "address": 250,
                    "data_type": "bool",
                    "bit_index": 0,
                    "value": false
                },
                {
                    "description": "读取设备当前配方id",
                    "operation_func": "read_update_sv",
                    "address": 248,
                    "data_type": "int",
                    "sv_name": "current_recipe_id"
                }
            ]
        },

        "carrier_in_request": {
            "loop": true,
            "description": "回流焊请求进站",
            "address": "2",
            "data_type": "int",
            "value": 1,
            "call_backs": [
                {
                    "description": "清空 eap 已回馈标识",
                    "operation_func": "clean_eap_reply",
                    "dv_name": "is_allow_carrier_in_reply"
                },
                {
                    "description": "读取回流焊托盘码",
                    "operation_func": "read_update_dv",
                    "address": "2",
                    "data_type": "str",
                    "dv_name": "carrier_code",
                    "event_name": "carrier_in_request"
                },
                {
                    "description": "等待 eap 回复回流焊请求进站",
                    "operation_func": "wait_eap_reply",
                    "dv_name": "is_allow_carrier_in_reply"
                },
                {
                    "description": "写入回流焊托盘是否能进站",
                    "operation_func": "write_dv_value",
                    "address": "D16011",
                    "data_type": "int",
                    "dv_name": "is_allow_carrier_in"
                },
                {
                    "description": "写入回流焊请求进站信号MES反馈信号",
                    "operation_func": "write_specify_value",
                    "address": "D16011",
                    "data_type": "int",
                    "value": -1
                }
            ]
        },
