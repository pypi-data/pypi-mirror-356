<odoo>
    <record id="view_export_csv_inscritions_wizard" model="ir.ui.view">
        <field name="name">export.csv.inscritions.wizard.form</field>
        <field name="model">export.csv.inscritions.wizard</field>
        <field name="arch" type="xml">
            <form string="Export CSV">
                <group>
                    <field name="file_data" filename="file_name" />
                    <field name="file_name" invisible="1"/>
                </group>
                <footer>
                    <button string="Export CSV" type="object" name="exportar_csv" class="btn-primary" attrs="{'invisible':[('file_data', '!=', False)]}"/>
                    <button string="Cancel" class="btn-secondary" special="cancel"/>
                </footer>
            </form>
        </field>
    </record>

    <record id="action_export_csv_inscritions_wizard" model="ir.actions.act_window">
        <field name="name">Export CSV</field>
        <field name="res_model">export.csv.inscritions.wizard</field>
        <field name="view_mode">form</field>
        <field name="target">new</field>
    </record>
</odoo>
