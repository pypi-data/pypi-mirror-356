{
  "$schema": "https://json.schemastore.org/package.json",
  "name": "airchief",
  "private": true,
  "type": "module",
  "packageManager": "bun@1.2.14",
  "scripts": {
    "typecheck": "bun run --filter='*' typecheck",
    "postinstall": "./scripts/hooks"
  },
  "workspaces": {
    "packages": [
      "packages/airchief"
    ],
    "catalog": {
      "typescript": "5.8.2",
      "@types/node": "22.13.9",
      "zod": "3.24.2",
      "ai": "4.3.16",
      "@tsconfig/node24": "24.0.0"
    }
  },
  "devDependencies": {
    "prettier": "3.5.3"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/shaneholloman/airchief"
  },
  "license": "MIT",
  "prettier": {
    "semi": false
  },
  "overrides": {
    "zod": "3.24.2"
  },
  "trustedDependencies": [
